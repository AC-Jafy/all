
#searchBar {
  width: 100%;
  height: 60rpx;
  position: fixed;
  top: 0;
  background-color: #2a2358;
  z-index: 999;
  overflow: hidden;
  display: flex;
}
#search {
  background-color: #fff;
  border-radius: 30rpx;
  display: inline-block;
  margin-top: 4px;
  flex: 1;
  padding-left: 20rpx;
}
#listALL {
  margin-top: 60rpx;
}
.kinds {
  width: 150rpx;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  border-right: 1px #ddd solid;
  border-bottom: 1px #ddd solid;
}
.list {
  background-color: khaki;
}